<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app_mainproject"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
    <uses-permission 
        android:name="android.permission.CAMERA"/>
    <uses-permission 
        android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission 
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.app_mainproject.AddEditContactActivity"
            android:label="@string/title_activity_add_edit_contact" 
            android:screenOrientation="landscape">
        </activity>
        
        <activity
            android:name="com.example.app_mainproject.TestActivity"
            android:label="@string/title_activity_test" 
            android:screenOrientation="landscape">
        </activity>
        
        <activity
            android:name="com.example.app_mainproject.Home"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.app_mainproject.OptionsMenu"
            android:label="@string/title_activity_options_menu"
            android:screenOrientation="landscape" >
        </activity>
    </application>

</manifest>
